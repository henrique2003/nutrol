{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "ios": {
        "distribution": "store"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": false,
      "autoIncrement": true
    }
  }
}
